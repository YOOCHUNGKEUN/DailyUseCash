<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_ffffff"
    android:orientation="vertical">


    <TextView
        android:id="@+id/dialog2_tv_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/dp_10"
        android:text="@string/init_data_title"
        style="@style/txt_20_ffffff"
        android:background="@color/color_127de4"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/dp_20">


        <TextView
            android:id="@+id/dialog2_tv_contents"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/txt_17_000000"
            android:textColorHint="@color/color_7d7d7d"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dp_10"
            android:gravity="right"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/dialog2_tv_cancel"
                style="@style/txt_17_127de4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_20"
                android:text="취소" />

            <TextView
                android:id="@+id/dialog2_tv_confirm"
                style="@style/txt_17_127de4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_20"
                android:text="확인" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>